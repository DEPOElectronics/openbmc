&i2c0 {
	status = "okay";
};

&i2c1 {
	status = "okay";
};

&i2c2 {
	/* i2c_pmbus */
	status = "okay";
	
	lm96163@4c {
		compatible = "national,lm96163";
		reg = <0x4c>;
	};	

};

&i2c3 {
	/* i2c_fruid */
	status = "okay";

	rtc@6f {
		compatible = "microchip,mcp7941x";
		reg = <0x6f>;
		interrupts = <110>;
	};
};

&i2c4 {
	/* i2c_cpu1 */
	status = "okay";
};

