&i2c0 {
	status = "okay";
};

&i2c1 {
	status = "okay";
};

&i2c2 {
	/* i2c_pmbus */
	status = "okay";
	
	lm96163@4c {
		compatible = "national,lm96163";
		reg = <0x4c>;
	};	

	rtc@6f {
		compatible = "microchip,mcp7941x";
		reg = <0x6f>;
		interrupts = <110>;
	};
};

&i2c3 {
	/* i2c_fruid */
	status = "okay";
};

&i2c4 {
	/* i2c_cpu1 */
	status = "okay";
	
    lm96163@4c {
        compatible = "national,lm96163";
        reg = <0x4c>;
        status = "okay";
        label = "CPU1 FAN Control";
        #address-cells = <0x1>;
        #size-cells = <0x0>;

        fan1: fan@1 {
            reg = <0x1>;
            label = "Fan";
            status = "okay";
            min_alarm = "200";
        };

        temp1: temp1@1 {
            reg = <0x1>;
            label = "Ambient";
            status = "okay";
            min_crit = "-40000";
            min_alarm = "-10000";
            max_alarm = "50000";
            max_crit = "80000";
        };

        temp@2 {
            reg = <0x2>;
            label = "not connected";
            status = "disabled";
        };
        
	pwm1: pwm@1 {
		#pwm-cells = <3>;
		status = "disabled";
	};
        
    };
};

&i2c5 {
	/* i2c_mntr */
	status = "okay";
};

&i2c6 {
	/* i2c_cpu0 */
	status = "okay";

	lm96163@4c {
		compatible = "national,lm96163";
		reg = <0x4c>;
	};	
};

&i2c7 {
	/* i2c_pci */
	status = "okay";
};

&i2c8 {
	status = "okay";
};

&i2c9 {
	status = "okay";
};

&i2c13 {
	status = "okay";
};
